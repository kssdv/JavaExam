package myproject01;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class exam02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner();
		String[] str_capa = sc.nextLine().split(" ");
//		int[] capa = {0,0,
		List<Integer> capa = new ArrayList<>();
		for(int i=0; i<str_capa.length; i++) {
			capa.add(Integer.parseInt(str_capa[i]));
		}
		System.out.println("capa[0]"+capa.get(0));
//		System.out.println("13+17 = " + (13 + 17));
//		System.out.println("13+17+77+89+25 = " + (13+17+77 + 89 + 25));
//		System.out.println((int)((0.0+13 + 17)/(0.0+13+17+77 + 89 + 25)*100));
	}

}
