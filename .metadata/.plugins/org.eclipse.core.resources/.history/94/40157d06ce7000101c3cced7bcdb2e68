package myproject01;

public class exam01 {

	public static void main(String[] args) {
		String line = "2 3 7";
        String[] lines = line.split(" ");
        int a = Integer.parseInt(lines[0]);
        int b = Integer.parseInt(lines[1]);
        int c = Integer.parseInt(lines[2]);
        
        if(a * b <= c){
            System.out.println("YES");
        }else{
            System.out.println("NO");
        }

	}

}
