package myproject01;

public class exam05 {

	public static void main(String[] args) {
		int line = 4;
		String[] lines = "2 4 3 1".split(" ");
		for(int i = 1; i <= line; i++) {
			for(int j=1; j<= Integer.parseInt(lines[i]); j++)) {
				if(j == 1) {
					System.out.print(j);
				}else {
					System.out.print(" "+j);
				}
			}
			System.out.println();
		}
	}

}
