package myproject01;

import java.util.Scanner;

public class exam01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        String[] str1 = sc.nextLine().split(" ");
        double s = Integer.parseInt(str1[0]);
        double a = Integer.parseInt(str1[1]);
        double t = Integer.parseInt(str1[2]);
        
        System.out.println((int) ((t*a)/(s*a) * 100));

	}

}
