		for(int i=0; i<num; i++) {
			Member3 member = new Member3();
			String[] temp = sc.nextLine().split(" ");
			String name = temp[0];
			boolean isDonate = temp[1].equals("give");
			member.name = name;
			int idx = findMemberIdx(name, members);
			
			if(isDonate) {
				int money = Integer.parseInt(temp[2]);
				if(idx >= 0) {
					Member3 m = members.get(idx);
					m.money += money;
					
					members.set(idx, m);
				}else {
					member.money = money;
					members.add(member);
				}
			}else {
				if(idx >= 0) {
					Member3 m = members.get(idx);
					m.subscribe = true;
					members.set(idx, m);
				}else {
					member.subscribe = true;
					subscribers.add(name);
					members.add(member);
				}
			}
//			members.add(member);
		}
		List<Member3> tmembers = new ArrayList<>();
//		for(int i=0; i<members.size(); i++) {
//			boolean flag = true;
//			for(int j=0; j<tmembers.size(); j++) {
//				if(members.get(i).name.equals(tmembers.get(j).name)) {
