package aRank;

public class exam10 {

	public static void main(String[] args) {
		for(int i=1; i<=5; i++) {
			int decimal = i;
			String binary = Integer.toBinaryString(decimal);
			System.out.println(binary);
		}
		System.out.println("==================");
		int x = 5; // Binary: 101
        int y = 3; // Binary: 011
        System.out.println("x ^ y = " + (x ^ y)); // Output: 6
		
	}

}
