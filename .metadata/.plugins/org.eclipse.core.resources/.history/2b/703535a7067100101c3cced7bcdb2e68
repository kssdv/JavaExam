package myproject01;

import java.util.Scanner;

public class exam08 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
//		System.out.println(String.format("%2d", 8));
		
		for(int i=1; i<=9; i++) {
			if(i > 1) {
				System.out.println("\n==========================================");
			}
			
			for(int j=1; j<=9; j++ ) {
				if(j > 1) {
					System.out.print(String.format("| %2d", i*j));
				}else {
					System.out.print(String.format("%2d ", i*j));
				}
			}
			
			
			
		}
		
	}

}
