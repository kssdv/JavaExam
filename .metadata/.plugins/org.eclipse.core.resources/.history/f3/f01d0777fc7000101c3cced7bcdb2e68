package myproject01;

import java.util.Scanner;

public class exam05 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		for (int i=0; i<num; i++) {
			String[] temp1 = sc.nextLine().split(" ");
			double temp2 = Double.parseDouble(temp1[0]);
			int temp3 = Integer.parseInt(temp1[1]);
			System.out.println(String.format("%."+temp3+"f", temp2));
		}
	}

}
