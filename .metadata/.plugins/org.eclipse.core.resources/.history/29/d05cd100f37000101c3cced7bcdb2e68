package myproject01;

public class exam05 {

	public static void main(String[] args) {
		int line = 8;
		for(int i=1; i<=line; i++) {
//			System.out.print(i);
			if(i <= 0 || line / 2 + 1 == i) {
				System.out.print(i);
			}else {
				System.out.print(" "+i);
			}
			if(line / 2 == i) {
				System.out.println();
			}
		}
	}

}
